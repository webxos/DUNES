{
  "version": 2,
  "name": "amoeba2048aes-orchestrator",
  "builds": [
    {
      "src": "orchestration_dunes.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.8"
      }
    }
  ],
  "routes": [
    {
      "src": "/execute-task",
      "dest": "orchestration_dunes.py"
    }
  ],
  "env": {
    "DROPBOX_ACCESS_TOKEN": "@dropbox_access_token",
    "DROPBOX_APP_KEY": "@dropbox_app_key",
    "DROPBOX_APP_SECRET": "@dropbox_app_secret",
    "CUDA_VERSION": "12.2"
  }
}