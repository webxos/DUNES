
{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "telescope.html",
      "use": "@vercel/static"
    },
    {
      "src": "server/mcp_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/mcp_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}
